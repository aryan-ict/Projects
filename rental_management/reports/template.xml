<odoo>
    <template id="rental_template">
<!--        <t t-call="web.html_container">-->
            <t t-foreach="docs" t-as="o">
<!--                <t t-call="web.external_layout">-->
                    <center>
                        <h1>Rental Management QWeb Report</h1>
                    </center>
                    <div class="page">
                        <h2>DATA</h2>
                        <br/>
                        <br/>
                        <ul>
                            <li>Name :
                                <span t-field="o.name"/>
                            </li>
                            <li>Customer :
                                <span t-field="o.partner_id"/>
                            </li>
                            <li>Rental type :
                                <span t-field="o.rental_type_id"/>
                            </li>
                            <li>Rental Product :
                                <span t-field="o.start_date"/>
                            </li>
                            <li>Start Date :
                                <span t-field="o.end_date"/>
                            </li>
                            <li>End Date :
                                <span t-field="o.rental_product"/>
                            </li>
                            <li>Price :
                                <span t-field="o.price"/>
                            </li>
                            <li>Status:
                                <span t-field="o.state"/>
                            </li>
                        </ul>
                    </div>
                </t>
<!--            </t>-->
<!--        </t>-->
    </template>

        <template id="rental_management.rental_main_template">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <t t-foreach="docs" t-as="doc">
                        <t t-call="rental_management.rental_template"/>
                    </t>
                </t>
            </t>
        </template>

</odoo>
