<odoo>

    <!-- Sub-Template -->
    <template id="rental_template">
        <t t-foreach="docs" t-as="o">
            <center>
                <h1>Rental Management QWeb Report</h1>
            </center>
            <div class="page">
                <h2>DATA</h2>
                <br/>
                <br/>
                <ul>
                    <li>Name :
                        <span t-field="o.name"/>
                    </li>
                    <li>Customer :
                        <span t-field="o.partner_id"/>
                    </li>
                    <li>Rental type :
                        <span t-field="o.rental_type_id"/>
                    </li>
                    <li>Rental Product :
                        <span t-field="o.start_date"/>
                    </li>
                    <li>Start Date :
                        <span t-field="o.end_date"/>
                    </li>
                    <li>End Date :
                        <span t-field="o.rental_product"/>
                    </li>
                    <li>Price :
                        <span t-field="o.price"/>
                    </li>
                    <li>Status:
                        <span t-field="o.state"/>
                    </li>
                    <table width="100%" style="border:1px solid black;border-collapse:collapse">
                        <tr>
                            <th>Name</th>
                            <th>Rental Type</th>
                            <th>Rental Product</th>
                            <th>Start Date</th>
                            <th>End Date</th>
                            <th>Price</th>
                            <th>Status</th>
                        </tr>

                        <t t-foreach="docs" t-as="a">
                            <tr>
                                <td>
                                    <span t-field="a.name"/>
                                </td>
                                <td>
                                    <span t-field="a.rental_type_id"/>
                                </td>
                                <td>
                                    <span t-field="a.rental_product"/>
                                </td>
                                <td>
                                    <span t-field="a.start_date"/>
                                </td>
                                <td>
                                    <span t-field="a.end_date"/>
                                </td>
                                <td>
                                    <span t-field="a.price"/>
                                </td>
                                <td>
                                    <span t-field="a.state"/>
                                </td>
                            </tr>
                        </t>

                    </table>
                </ul>
            </div>
        </t>
    </template>

    <!-- Main-Template -->
    <template id="rental_management.rental_main_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="rental_management.rental_template"/>
            </t>
        </t>
    </template>

</odoo>
